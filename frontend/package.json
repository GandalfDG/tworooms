{
  "name": "tworooms-vue",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "stackdev": "vite & cd ../tworooms-sanic; sanic --dev -p 1337 server.app",
    "build": "vite build",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest run --dom",
    "css-build": "node-sass --omit-source-map-url src/assets/bulma_overrides.scss src/assets/bulma_style.css",
    "css-watch": "npm run css-build -- --watch"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bulma": "^0.9.4",
    "node-sass": "^7.0.1",
    "pinia": "^2.0.14",
    "vue": "^3.2.36",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^2.0.0",
    "cypress": "^10.3.1",
    "eslint": "^8.25.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-vue": "^9.6.0",
    "happy-dom": "^6.0.4",
    "jsdom": "^19.0.0",
    "prettier": "^2.5.1",
    "start-server-and-test": "^1.14.0",
    "vite": "^2.9.9",
    "vitest": "^0.21.1"
  }
}
